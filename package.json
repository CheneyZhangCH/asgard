{
  "private": true,
  "scripts": {
    "start": "UMI_ENV=dev umi dev",
    "build": "UMI_ENV=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier  --write"
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@rematch/core": "^1.4.0",
    "@rematch/loading": "^1.2.1",
    "@types/uuid": "^7.0.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.2",
    "antd": "^4.1.5",
    "axios": "^0.19.2",
    "hotkeys-js": "^3.7.6",
    "lint-staged": "^10.0.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.1.2",
    "uuid": "^7.0.3",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.6",
    "prettier": "^1.19.1"
  }
}
